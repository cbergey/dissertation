@book{katsos_pragmatic_nodate,
	abstract = {It's not enough to tell the truth if it is not the whole truth. This is a fundamental conversational principle that arises out of pragmatic considerations of how much information interlocutors are expected to exchange (see Grice, 1989, as well as Carston, 1998; Hirschberg, 1991; Horn, 1984; Levinson, 2000; Sperber \& Wilson, 1986/1995 i.a.). But how do young children come to master this skill? One way of investigating this involves studying whether children object to other people's utterances that are only partially true. To illustrate this, take an experimental situation like (1), where Mr Caveman is a fictional character that participants help to learn English. (1) Scenario: Experimenter, Mr Caveman, and participant watch a short story where the dog, who is an artist, paints the triangle and the square. a. Experimenter to Mr Caveman: What did the dog paint? b. Mr Caveman: The dog painted the triangle. c. Experimenter to participant: Did he say it right? Mr Caveman's answer in (1b) is grammatically flawless and logically true (because the triangle is indeed one of the things that the dog painted). However, it is pragmatically infelicitous, because Mr Caveman gave less information than he could. Moreover, the utterance could be interpreted with a quantity implicature, to the effect that `The dog painted the triangle but not the square', which would be inappropriate in the situation at hand. The flourishing literature on under-informativeness and implicature documents that when children are asked to evaluate utterances that are only partially true, they start from a stage where they tend to accept them, and only gradually come to reject them as adults},
	author = {Katsos, Napoleon and Smith, Nafsika},
	file = {Citeseer - Full Text PDF:/Users/clairebergey/Zotero/storage/47IIGG2G/Katsos and Smith - Pragmatic Tolerance or a Speaker-Comprehender Asym.pdf:application/pdf;Citeseer - Snapshot:/Users/clairebergey/Zotero/storage/9PQU7V5F/summary.html:text/html},
	title = {Pragmatic {Tolerance} or a {Speaker}-{Comprehender} {Asymmetry} in the {Acquisition} of {Informativeness}?}}

@article{beck_can_2008,
	abstract = {In two experiments, we examined young children's ability to delay a response to ambiguous input. In Experiment 1, 5- and 6-year-olds performed as poorly when they needed to choose between basing an interpretation on ambiguous input and delaying an interpretation as when making explicit evaluations of knowledge, whereas 7- and 8-year-olds found the former task easy. In Experiment 2, 5- and 6-year-olds performed well on a task that required delaying a response but removed the need to decide between strategies. We discuss children's difficulty with ambiguity in terms of the decision-making demands made by different procedures. These demands appear to cause particular problems for young children.},
	author = {Beck, Sarah R. and Robinson, Elizabeth J. and Freeth, Megan M.},
	file = {ScienceDirect Full Text PDF:/Users/clairebergey/Zotero/storage/5S5NBPII/Beck et al. - 2008 - Can children resist making interpretations when un.pdf:application/pdf;ScienceDirect Snapshot:/Users/clairebergey/Zotero/storage/T57V6IMR/S002209650700080X.html:text/html},
