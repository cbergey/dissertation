	shorttitle = {Generating {Referring} {Expressions}},
	title = {Generating {Referring} {Expressions}: {Making} {Referents} {Easy} to {Identify}},
	urldate = {2018-12-14},
	volume = {33},
	year = {2007}}

@article{gweon_sins_2014,
	abstract = {Do children know when people tell the truth but not the whole truth? Here we show that children accurately evaluate informants who omit information and adjust their exploratory behavior to compensate for under-informative pedagogy. Experiment 1 shows that given identical demonstrations of a toy, children (6- and 7-year-olds) rate an informant lower if the toy also had non-demonstrated functions. Experiment 2 shows that given identical demonstrations, six-year-olds explore a toy more broadly if the informant previously committed a sin of omission. These results suggest that children consider both accuracy and informativeness in evaluating others' credibility and adjust their exploratory behavior to compensate for under-informative testimony when an informant's credibility is in doubt.},
	author = {Gweon, Hyowon and Pelton, Hannah and Konopka, Jaclyn A. and Schulz, Laura E.},
	file = {ScienceDirect Full Text PDF:/Users/clairebergey/Zotero/storage/8NM57QGD/Gweon et al. - 2014 - Sins of omission Children selectively explore whe.pdf:application/pdf;ScienceDirect Snapshot:/Users/clairebergey/Zotero/storage/XXTN5X4M/S0010027714000821.html:text/html},
